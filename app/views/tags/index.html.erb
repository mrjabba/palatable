<h1>Tag Repository</h1>
<div class="actions">
  <nav class="round">
    <ul>
      <li><%= link_to "New Tag", new_tag_path, :class => "action_button round" %></li>
    </ul>
  </nav>
</div>

<%= form_tag tags_path, :method => 'get', :remote => true, :id => "tags_search" do %>
  <p>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search", :name => nil %>
  </p>

  <div id="tags">
    <%= render 'tag' %>
  </div>

<% end %>


