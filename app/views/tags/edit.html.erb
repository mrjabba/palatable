<h1>Edit tag</h1>

<%= form_for(@tag) do |f| %>
  <%= render 'shared/error_messages_tag', :object => f.object %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

  <div class="actions">
    <%= f.submit "Update", :class => "button round" %>
    <input type="button" id="cancelTagUpdate" value="Cancel" class="button round" />
  </div>
<% end %>

