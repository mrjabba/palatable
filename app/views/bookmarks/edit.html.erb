<h1>Edit bookmark</h1>

<%= form_for(@bookmark) do |f| %>
  <%= render 'shared/error_messages_bookmark', :object => f.object %>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </div>
  <div class="field">
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </div>

  <div class="actions">
    <%= f.submit "Update", :class => "button round" %>
    <input type="button" id="cancelBookmarkUpdate" value="Cancel" class="button round" />
  </div>
<% end %>

